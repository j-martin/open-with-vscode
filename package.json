{
  "name": "openwith",
  "displayName": "OpenWith",
  "description": "Open current file in any application or text editor.",
  "version": "0.3.1",
  "publisher": "jmarcher",
  "repository": "https://github.com/j-martin/open-with-vscode",
  "engines": {
    "vscode": ">=1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "contributes": {},
    "commands": [
      {
        "command": "open-with.open",
        "title": "Open With..."
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.97.0",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "@vscode/vsce": "^3.2.2",
    "eslint": "^9.19.0",
    "typescript": "^5.7.3"
  }
}
